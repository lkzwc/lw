
.container.data-v-1c7c41d0 {
		background-color: #f8f8f8;
		min-height: 100vh;
		padding-bottom: 120rpx;
}

	/* 搜索区域 */
.search-section.data-v-1c7c41d0 {
		background-color: white;
		padding: 24rpx;
		margin-bottom: 16rpx;
}
.search-box.data-v-1c7c41d0 {
		display: flex;
		align-items: center;
		background-color: #f5f5f5;
		border-radius: 50rpx;
		padding: 16rpx 24rpx;
}
.search-input.data-v-1c7c41d0 {
		flex: 1;
		margin-left: 16rpx;
		font-size: 28rpx;
		color: #333;
}
.search-btn.data-v-1c7c41d0 {
		background-color: #007aff;
		color: white;
		padding: 12rpx 24rpx;
		border-radius: 30rpx;
		margin-left: 16rpx;
}
.search-btn text.data-v-1c7c41d0 {
		font-size: 24rpx;
		color: white;
}

	/* 筛选区域 */
.filter-section.data-v-1c7c41d0 {
		background-color: white;
		padding: 24rpx 0;
		margin-bottom: 16rpx;
}
.filter-scroll.data-v-1c7c41d0 {
		white-space: nowrap;
}
.filter-item.data-v-1c7c41d0 {
		display: inline-block;
		padding: 12rpx 24rpx;
		margin: 0 12rpx;
		border-radius: 30rpx;
		background-color: #f5f5f5;
		color: #666;
		font-size: 24rpx;
		transition: all 0.3s;
}
.filter-item.active.data-v-1c7c41d0 {
		background-color: #007aff;
		color: white;
}

	/* 技能列表 */
.skills-list.data-v-1c7c41d0 {
		padding: 0 24rpx;
}
.skill-item.data-v-1c7c41d0 {
		background-color: white;
		border-radius: 16rpx;
		padding: 24rpx;
		margin-bottom: 24rpx;
		box-shadow: 0 4rpx 12rpx rgba(0, 0, 0, 0.05);
}

	/* 技能头部 */
.skill-header.data-v-1c7c41d0 {
		display: flex;
		align-items: center;
		margin-bottom: 20rpx;
}
.user-avatar.data-v-1c7c41d0 {
		width: 80rpx;
		height: 80rpx;
		border-radius: 50%;
		margin-right: 20rpx;
}
.user-info.data-v-1c7c41d0 {
		flex: 1;
}
.username.data-v-1c7c41d0 {
		font-size: 28rpx;
		font-weight: 600;
		color: #333;
		display: block;
		margin-bottom: 8rpx;
}
.location.data-v-1c7c41d0 {
		display: flex;
		align-items: center;
}
.location-text.data-v-1c7c41d0 {
		font-size: 22rpx;
		color: #999;
		margin-left: 8rpx;
}
.price.data-v-1c7c41d0 {
		text-align: right;
}
.price-text.data-v-1c7c41d0 {
		font-size: 32rpx;
		font-weight: 700;
		color: #ff4757;
}
.price-unit.data-v-1c7c41d0 {
		font-size: 22rpx;
		color: #999;
}

	/* 技能图片 */
.skill-images.data-v-1c7c41d0 {
		display: flex;
		margin-bottom: 20rpx;
		gap: 12rpx;
}
.skill-image.data-v-1c7c41d0 {
		width: 200rpx;
		height: 150rpx;
		border-radius: 12rpx;
}
.more-images.data-v-1c7c41d0 {
		width: 200rpx;
		height: 150rpx;
		border-radius: 12rpx;
		background-color: rgba(0, 0, 0, 0.6);
		display: flex;
		align-items: center;
		justify-content: center;
		color: white;
		font-size: 24rpx;
}

	/* 技能内容 */
.skill-content.data-v-1c7c41d0 {
		margin-bottom: 20rpx;
}
.skill-title.data-v-1c7c41d0 {
		font-size: 30rpx;
		font-weight: 600;
		color: #333;
		display: block;
		margin-bottom: 12rpx;
}
.skill-desc.data-v-1c7c41d0 {
		font-size: 26rpx;
		color: #666;
		line-height: 1.5;
		display: block;
		margin-bottom: 16rpx;
}
.skill-tags.data-v-1c7c41d0 {
		display: flex;
		flex-wrap: wrap;
		gap: 12rpx;
}
.tag.data-v-1c7c41d0 {
		background-color: #e8f4fd;
		color: #007aff;
		padding: 8rpx 16rpx;
		border-radius: 20rpx;
		font-size: 22rpx;
}

	/* 技能底部 */
.skill-footer.data-v-1c7c41d0 {
		display: flex;
		justify-content: space-between;
		align-items: center;
}
.rating.data-v-1c7c41d0 {
		display: flex;
		align-items: center;
}
.rating-text.data-v-1c7c41d0 {
		font-size: 24rpx;
		color: #333;
		margin-left: 8rpx;
}
.review-count.data-v-1c7c41d0 {
		font-size: 22rpx;
		color: #999;
		margin-left: 8rpx;
}
.publish-time.data-v-1c7c41d0 {
		font-size: 22rpx;
		color: #999;
}

	/* 空状态 */
.empty-state.data-v-1c7c41d0 {
		text-align: center;
		padding: 120rpx 40rpx;
}
.empty-text.data-v-1c7c41d0 {
		font-size: 28rpx;
		color: #999;
		display: block;
		margin: 24rpx 0 12rpx;
}
.empty-desc.data-v-1c7c41d0 {
		font-size: 24rpx;
		color: #ccc;
		display: block;
}

	/* 加载状态 */
.loading-state.data-v-1c7c41d0 {
		padding: 40rpx;
}

	/* 添加技能按钮 */
.add-skill-btn.data-v-1c7c41d0 {
		position: fixed;
		right: 40rpx;
		bottom: 120rpx;
		width: 100rpx;
		height: 100rpx;
		background-color: #007aff;
		border-radius: 50%;
		display: flex;
		align-items: center;
		justify-content: center;
		box-shadow: 0 8rpx 24rpx rgba(0, 122, 255, 0.3);
		z-index: 999;
}
